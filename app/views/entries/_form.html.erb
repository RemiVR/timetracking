<%= form_for [@project, @entry], :html => { :multipart => true } do |f|%>
	<%= f.label :hours%>
	<%= f.text_field :hours%>
	<br>
	<%= f.label :minutes%>
	<%= f.text_field :minutes%>
	<br>
	<%= f.label :date%>
	<%= f.text_field :date%>
	<br>
	<%= f.label :comments%>
	<%= f.text_field :comments%>
	<br>
	<%= f.submit 'save'%>
<%end%>