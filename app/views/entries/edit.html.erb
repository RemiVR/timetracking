<%if @entry.errors.any? %>
	<ul>
		<% @entry.errors.full_messages.each do |errors_msg| %>
		<li><%= errors_msg %></li>
		<% end %>
	</ul>
<% end %>

<%= form_for [@project, @entry] do |f|%>
	<%= f.label :hours%>
	<%= f.text_field :hours, size: 4%>
	<br>
	<%= f.label :minutes%>
	<%= f.text_field :minutes, size: 4%>
	<br>
	<%= f.label :date%>
	<%= f.text_field :date%>
	<br>
	<%= f.label :comments%>
	<%= f.text_field :comments%>
	<br>
	<%= f.submit 'save'%>
<%end%>
<br>
<h4><%= link_to("Cancel", project_entries_path)%></h4>